#!groovy
pipeline{
 agent any
 
  tools {
    maven 'maven'
  }
 
 stages{
 
   stage("build"){
   
       steps{
       
          bat "mvn -f ./invapidocker clean install -DskipTests"
          bat "cd invapidocker"
          bat "docker-compose up"                          
       }
   
   }
 
 }
 
 
 
}

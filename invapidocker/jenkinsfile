#!groovy
pipeline{
 agent any
 
  tools {
    maven 'maven'
  }
 
 stages{
 
   stage("build"){
   
       steps{
       
          bat "mvn -f ./invapidocker clean install -DskipTests"
          cd invapidocker
          bat "docker-compose up"                          
       }
   
   }
 
 }
 
 
 
}
